{
    "version": 2,
    "builds": [
        {
            "src": "ejeh_palace/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "ejeh_palace/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "ejeh_palace.settings",
        "VERCEL": "1"
    }
}
